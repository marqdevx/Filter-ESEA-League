{
    "manifest_version": 2,
    "name": "ESEA filter",
    "description": "A basic extension to ESEA seasons' ladderboards!",
    "version": "1.2",
    "author": "https://github.com/marqdevx",
    "permissions": [
        "https://play.esea.net/league/standings*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://play.esea.net/league/standings",
                "https://play.esea.net/league/standings*"
            ],
            "js": [
                "eseaFilter.js"
            ]
        }
    ]
}